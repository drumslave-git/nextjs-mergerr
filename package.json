    "@next/bundle-analyzer": "15.0.3",
    "@prisma/client": "^6.0.1",
    "axios": "^1.7.7",
    "concurrently": "^8.2.2",
    "fluent-ffmpeg": "^2.1.3",
    "next": "15.0.3",
    "qs": "^6.12.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
